                                    <sio:SIO_000300>1.2.4 Cabled observatories</sio:SIO_000300>
                                  </sio:SIO_000175>
                                </sio:SIO_000008>
                                <sio:SIO_000068 rdf:resource="http://bio2rdf.org/pmc:118352054791268"/>
