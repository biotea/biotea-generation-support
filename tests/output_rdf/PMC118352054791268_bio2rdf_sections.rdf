                                              <rdf:Seq rdf:about="http://bio2rdf.org/pmc_resource:118352054791268_listOfParagraphs_list20">
                                                <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_Take-pictures-of-recovered-sensor-as-is-for-metadata-_para_1"/>
                                                <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_Take-pictures-of-recovered-sensor-as-is-for-metadata-_para_2"/>
                                                <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_Take-pictures-of-recovered-sensor-as-is-for-metadata-_para_3"/>
                                                <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_Take-pictures-of-recovered-sensor-as-is-for-metadata-_para_4"/>
