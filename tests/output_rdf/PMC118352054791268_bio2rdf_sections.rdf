                                        <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_section_1-2-Platform-types--CNR--IFREMER--INGV--IEO-_1-2-4-Cabled-observatories"/>
                                        <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_section_1-2-Platform-types--CNR--IFREMER--INGV--IEO-_1-2-5-Platform-summary"/>
