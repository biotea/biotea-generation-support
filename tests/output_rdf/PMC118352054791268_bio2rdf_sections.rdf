                                        <sio:SIO_000028 rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-1-R-V-operations_para_1"/>
                                        <sio:SIO_000068 rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_section_1-3-Design-structure--OGS--GEOMAR--PLOCAN-"/>
