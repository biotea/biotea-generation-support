                                          </rdf:Seq>
                                        </sio:SIO_000008>
                                        <sio:SIO_000008>
                                          <sio:SIO_000175 rdf:about="http://bio2rdf.org/pmc_resource:118352054791268_other_dcterms_title_node31_1507641546482">
                                            <sio:SIO_000300>3.2.3 Deployment procedures</sio:SIO_000300>
                                          </sio:SIO_000175>
                                        </sio:SIO_000008>
                                        <sio:SIO_000008>
                                          <rdf:Seq rdf:about="http://bio2rdf.org/pmc_resource:118352054791268_listOfParagraphs_list17">
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_1"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_2"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_3"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_4"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_5"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_6"/>
                                            <rdf:li rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-3-Design-structure--OGS--GEOMAR--PLOCAN-_3-2-3-Deployment-procedures_para_7"/>
