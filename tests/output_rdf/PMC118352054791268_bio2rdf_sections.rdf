                                        <sio:SIO_000028 rdf:resource="http://bio2rdf.org/pmc_resource:118352054791268_paragraph_1-2-Platform-types--CNR--IFREMER--INGV--IEO-_1-2-1-Mooring-lines_para_1"/>
